   <!--  this is our beans -->    
    <bean class="com.springcore.student" name="student1">
    	<property name="studentId" value="22344">
    	</property>    	
    	<property name="studentName" value="John">
    		
    	</property>
    	
    	<property name="studentAddress" value="Lucknow">
    		
    	</property>
    
    </bean>
        <bean class="com.springcore.student" name="student2">
    	<property name="studentId" value="22345">
    	</property>    	
    	<property name="studentName" value="karun">
    		
    	</property>
    	
    	<property name="studentAddress" value="Sangrur">
    		
    	</property>
    
    </bean>
    
 
        <bean class="com.springcore.student" name="student3" p:studentId="24234" p:studentName="Ahuja" p:studentAddress="Sangrur">
    	
    
    </bean>
    
  List  
    <bean>
    <property name="">
    <list>
    <value>10</value>
    
    <value>1023</value>
    
    <value>10222</value>
    
    
    <value>1043</value>
    
    
    </list>
    
    </property>
    
    </bean>
    
    Set  
    <bean>
    <property name="">
    <set>
    <value>10</value>
    
    <value>1023</value>
    
    <value>10222</value>
    
    
    <value>1043</value>
    
    
    </set>
    
    </property>
    
    </bean>
    
    
    Map
   
    <bean>
    <property name="">
    <map>
    <entry key="java" value="2month"/>
     <entry key="python" value="1month"/>
    
    
    </map>
    
    </property>
    
    </bean>
    
  properties  
    
  
    <bean>
    <property name="">
    <props>
    
    
     <prop.key="java">Karun</prop>
     <prop.key="python">Ahuja</prop>
    
    
    </props>
   
  </property>
    
</bean>
    
</beans>